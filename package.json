{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.development nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node build/index.js",
    "clean": "rm -rf build",
    "test": "tap --ts --reporter=list --no-check-coverage tests/**/*.ts",
    "prisma:dev": "dotenv -e .env.development npx prisma",
    "prisma:test": "dotenv -e .env.test npx prisma",
    "prisma:push": "dotenv -e .env.development npx prisma db push",
    "prisma:seed": "yarn prisma:push && yarn prisma:dev db seed"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.9",
    "@types/tap": "^15.0.5",
    "@types/ua-parser-js": "^0.7.36",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.8.1",
    "prisma-dbml-generator": "^0.8.3",
    "tap": "^16.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.51.0",
    "@aws-sdk/s3-request-presigner": "^3.51.0",
    "@bull-board/fastify": "^3.10.2",
    "@prisma/client": "^3.12.0",
    "@types/bull": "^3.15.8",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^14.2.0",
    "ezly-render-html": "^4.1.0",
    "fastify": "^3.26.0",
    "fastify-cookie": "^5.5.0",
    "fastify-cors": "^6.0.2",
    "fastify-plugin": "^3.0.0",
    "fastify-request-context": "^2.2.0",
    "fastify-sensible": "^3.1.2",
    "fastify-swagger": "^4.13.1",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.2.0",
    "pino-pretty": "^7.5.0",
    "tedis": "^0.1.12",
    "ua-parser-js": "^1.0.2",
    "validator": "^13.7.0"
  },
  "prisma": {
    "seed": "dotenv -e .env.development ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
